{
    "name": "babdev/supplier-plugin",
    "type": "sylius-plugin",
    "description": "Supplier processing for Sylius.",
    "keywords": [
        "shop",
        "ecommerce",
        "supplier"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "sylius/sylius": "~1.11.2",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/form": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/options-resolver": "^5.4"
    },
    "require-dev": {
        "behat/behat": "^3.6.1",
        "behat/mink-selenium2-driver": "^1.4",
        "dmore/behat-chrome-extension": "^1.3",
        "dmore/chrome-mink-driver": "^2.7",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-debug-extension": "^2.0.0",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/variadic-extension": "^1.3",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "phpspec/phpspec": "^7.1",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "1.10.43",
        "phpstan/phpstan-doctrine": "1.3.53",
        "phpstan/phpstan-strict-rules": "1.5.2",
        "phpstan/phpstan-webmozart-assert": "1.2.4",
        "phpunit/phpunit": "9.6.13",
        "polishsymfonycommunity/symfony-mocker-container": "^1.0",
        "sylius-labs/coding-standard": "^4.3.1",
        "symfony/browser-kit": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/intl": "^5.4",
        "symfony/web-profiler-bundle": "^5.4",
        "symfony/webpack-encore-bundle": "^1.14",
        "vimeo/psalm": "4.30.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "symfony/thanks": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "BabDev\\SyliusSupplierPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BabDev\\SyliusSupplierPlugin\\Tests\\App\\": "tests/Application/src/",
            "Tests\\BabDev\\SyliusSupplierPlugin\\": "tests/"
        },
        "classmap": [
            "tests/Application/Kernel.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-update-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-create-project-cmd": [
            "php bin/create_node_symlink.php"
        ]
    }
}
