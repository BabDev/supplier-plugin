{
    "name": "babdev/supplier-plugin",
    "type": "sylius-plugin",
    "description": "Supplier processing for Sylius.",
    "keywords": ["shop", "ecommerce", "supplier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Babker",
            "email": "michael.babker@gmail.com",
            "homepage": "https://michaels.website"
        }
    ],
    "require": {
        "php": "^7.1",

        "sylius/sylius": "^1.0.4"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/context-service-extension": "^1.0",
        "friends-of-behat/cross-container-extension": "^1.0",
        "friends-of-behat/service-container-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^1.0",
        "friends-of-behat/variadic-extension": "^1.0",
        "lakion/mink-debug-extension": "^1.2.3",
        "phpspec/phpspec": "^3.2",
        "phpstan/phpstan-shim": "^0.9.2",
        "phpunit/phpunit": "^6.5",
        "se/selenium-server-standalone": "^2.52",
        "sylius-labs/coding-standard": "^1.0",
        "symplify/easy-coding-standard": "^2.4"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "BabDev\\SyliusSupplierPlugin\\": "src/" }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/app/AppKernel.php"],
        "psr-4": {
            "BabDev\\SyliusSupplierPlugin\\Tests\\App\\": "tests/Application/src/",
            "BabDev\\SyliusSupplierPlugin\\Tests\\Behat\\": "tests/Behat/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
