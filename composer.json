{
    "name": "babdev/supplier-plugin",
    "type": "sylius-plugin",
    "description": "Supplier processing for Sylius.",
    "keywords": ["shop", "ecommerce", "supplier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Babker",
            "email": "michael.babker@gmail.com",
            "homepage": "https://michaels.website"
        }
    ],
    "require": {
        "php": "^7.1",

        "sylius/sylius": "^1.0@beta",
        "symfony/symfony": "^3.2"
    },
    "require-dev": {
        "jackalope/jackalope-doctrine-dbal": "^1.2",

        "behat/behat": "^3.2",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/context-service-extension": "^0.3",
        "friends-of-behat/cross-container-extension": "^0.2",
        "friends-of-behat/performance-extension": "^1.0",
        "friends-of-behat/service-container-extension": "^0.3",
        "friends-of-behat/symfony-extension": "^0.2.1",
        "friends-of-behat/variadic-extension": "^0.1",
        "lakion/mink-debug-extension": "^1.2.3",
        "phpspec/phpspec": "^3.2",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": { "BabDev\\SupplierPlugin\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "BabDev\\SupplierPlugin\\Tests\\Behat\\": "tests/Behat/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
